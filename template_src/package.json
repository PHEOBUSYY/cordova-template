{
  "name": "com.justyan.corodva.template",
  "displayName": "cordova-template",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -p --config webpack.prod.js",
    "dll": "webpack -p --config webpack.dll.js",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --open --content-base ./src --config webpack.dev.js",
    "mockenv": "webpack-dev-server --devtool eval --progress --colors --hot --open --content-base ./src --config webpack.mock.js",
    "mock": "json-server --watch src/mock/db.json --routes src/mock/routes.json",
    "mockdev": "npm run mock & npm run dev"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "json-server": "^0.12.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "cordova": "^8.0.0",
    "babel-polyfill": "^6.26.0",
    "axios": "^0.16.2",
    "cordova-android": "^6.2.3",
    "cordova-ios": "^4.4.0",
    "cordova-plugin-whitelist": "^1.3.2",
    "framework7": "^1.6.5",
    "framework7-vue": "^0.9.3",
    "vue": "^2.4.4",
    "vue-axios": "^2.0.2",
    "vue-toasted": "^1.1.22",
    "vuex": "^2.3.1",
    "webpack": "^3.6.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [],
    "rules": {
      "max-len": [
        1,
        170,
        2,
        {
          "ignoreComments": true
        }
      ],
      "eqeqeq": 1
    }
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
